{
    "name": "landingi/php-coding-standards",
    "description": "Landingi Static Code Analyze Bundle",
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "phpstan/phpstan": "0.12.99",
        "phpstan/phpstan-doctrine": "0.12.44",
        "phpstan/phpstan-phpunit": "0.12.22",
        "phpstan/phpstan-symfony": "0.12.44",
        "symplify/easy-coding-standard": "9.4.22",
        "symfony/polyfill-php80": "^1.23"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "symfony/dotenv": "^5.3",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Landingi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Landingi\\": "tests/"
        }
    }
}
